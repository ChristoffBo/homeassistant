{
  "name": "Addons Updater Enhanced",
  "version": "3.0.1",
  "slug": "addons_updater_enhanced",
  "description": "Automatically update addons with notifications support",
  "url": "https://github.com/ChristoffBo/homeassistant/tree/main/addons_updater",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "application",
  "boot": "manual",
  "init": false,
  "hassio_api": true,
  "hassio_role": "manager",
  "map": ["config:rw", "backup:rw"],
  "options": {
    "gituser": "Your Name",
    "gitmail": "your@email.com",
    "gitapi": "",
    "repository": "your-username/your-repo",
    "verbose": false,
    "dry_run": false,
    "enable_notifications": false,
    "gotify_url": "",
    "gotify_token": "",
    "gitea_api_url": "",
    "gitea_token": ""
  },
  "schema": {
    "gituser": "str",
    "gitmail": "str?",
    "gitapi": "str?",
    "repository": "str",
    "verbose": "bool",
    "dry_run": "bool",
    "enable_notifications": "bool",
    "gotify_url": "str?",
    "gotify_token": "str?",
    "gitea_api_url": "str?",
    "gitea_token": "str?"
  }
}
