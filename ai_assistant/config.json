{
  "name": "AI Assistant",
  "version": "4.3.1",
  "slug": "ai_assistant",
  "description": "AI Assistant with PID-1 compatibility fixes",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "system",
  "boot": "auto",
  "init": true,
  "pid": "host",
  "host_network": false,
  "host_pid": false,
  "host_ipc": false,
  "host_dbus": false,
  "privileged": [],
  "full_access": false,
  "apparmor": false,
  "devices": [],
  "auto_uart": false,
  "ports": {
    "5000/tcp": 5000
  },
  "ports_description": {
    "5000/tcp": "Web interface"
  },
  "ingress": true,
  "ingress_port": 5000,
  "ingress_entry": "/",
  "panel_icon": "mdi:robot",
  "panel_title": "AI Assistant",
  "panel_admin": false,
  "environment": {
    "S6_CMD_WAIT_FOR_SERVICES": "1",
    "S6_CMD_WAIT_FOR_SERVICES_MAXTIME": "30000",
    "S6_SERVICES_GRACETIME": "30000"
  },
  "map": ["config:rw"],
  "options": {
    "openai_key": "",
    "model": "gpt-3.5-turbo",
    "api_base": "https://api.openai.com/v1",
    "temperature": 0.7
  },
  "schema": {
    "openai_key": "str?",
    "model": "str?",
    "api_base": "str?",
    "temperature": "float(0,2)?"
  }
}