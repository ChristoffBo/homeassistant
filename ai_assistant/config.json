{
  "name": "AI Assistant",
  "version": "1.0.0",
  "slug": "ai_assistant",
  "description": "Cloud AI Assistant (OpenAI/DeepSeek API)",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 5000,
  "panel_icon": "mdi:robot",
  "panel_title": "AI Assistant",
  "options": {
    "provider": "openai",  // "openai" or "deepseek"
    "api_key": "your-api-key-here",
    "api_base": "https://api.openai.com/v1",  // For DeepSeek: "https://api.deepseek.com/v1"
    "model": "gpt-3.5-turbo",  // For DeepSeek: "deepseek-chat"
    "temperature": 0.7,
    "max_tokens": 512
  },
  "schema": {
    "provider": "match(openai|deepseek)",
    "api_key": "str",
    "api_base": "str",
    "model": "str",
    "temperature": "float(0,2)?",
    "max_tokens": "int(128,2048)?"
  },
  "environment": {
    "LOG_LEVEL": "debug"
  }
}