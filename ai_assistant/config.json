{
  "name": "AI Assistant",
  "version": "3.0",
  "slug": "ai_assistant",
  "description": "Stable version with PID 1 fix",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "5000/tcp": 5000
  },
  "map": ["config:rw"],
  "options": {
    "openai_key": "",
    "github_token": ""
  },
  "schema": {
    "openai_key": "str?",
    "github_token": "str?"
  },
  "webui": "http://[HOST]:[PORT:5000]",
  "ingress": true,
  "ingress_port": 5000,
  "panel_icon": "mdi:robot",
  "panel_title": "AI Assistant"
}