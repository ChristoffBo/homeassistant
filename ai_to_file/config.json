{
  "name": "AI to File",
  "version": "2.1.0",
  "slug": "ai_to_file",
  "description": "Chat with AI models and export conversations to files",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "options": {
    "port": 5000,
    "default_model": "chatgpt",
    "chatgpt_type": "free",
    "chatgpt_api_key": "",
    "deepseek_type": "free",
    "deepseek_api_key": ""
  },
  "schema": {
    "port": "int(1,65535)",
    "default_model": "match(chatgpt|deepseek)",
    "chatgpt_type": "match(free|pro|custom)",
    "chatgpt_api_key": "str?",
    "deepseek_type": "match(free|pro|custom)",
    "deepseek_api_key": "str?"
  },
  "ports": {
    "5000/tcp": 5000
  },
  "ports_description": {
    "5000/tcp": "Web interface port"
  },
  "webui": "http://[HOST]:[PORT:5000]"
}