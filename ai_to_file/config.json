{
    "name": "AI to File",
    "version": "2.1.0",
    "slug": "ai_to_file",
    "description": "Chat with AI models and export conversations to files",
    "arch": ["amd64", "armv7", "aarch64"],
    "startup": "application",
    "boot": "auto",
    "options": {
        "port": 5000,
        "default_model": "chatgpt",
        "apis": {
            "chatgpt": {
                "type": "free",
                "api_key": ""
            },
            "deepseek": {
                "type": "free",
                "api_key": ""
            }
        }
    },
    "schema": {
        "port": "int",
        "default_model": "match(chatgpt|deepseek)",
        "apis": {
            "chatgpt": {
                "type": "match(free|pro|custom)",
                "api_key": "str?"
            },
            "deepseek": {
                "type": "match(free|pro|custom)",
                "api_key": "str?"
            }
        }
    },
    "ports": {
        "5000/tcp": "{{port}}"
    },
    "ports_description": {
        "5000/tcp": "Web interface port"
    },
    "webui": "http://[HOST]:[PORT:5000]"
}
