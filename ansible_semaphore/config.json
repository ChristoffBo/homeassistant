{
  "name": "Ansible Semaphore",
  "version": "v2.16.17",
  "slug": "ansible_semaphore",
  "description": "Modern web UI for Ansible automation with a BoltDB (SQLite-like) backend. Persistent storage under /share/semaphore.",
  "url": "https://github.com/semaphoreui/semaphore",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "map": ["share:rw", "ssl:ro"],
  "options": {
    "admin_user": "admin",
    "admin_email": "admin@example.com",
    "admin_password": "changeme",
    "log_level": "info",
    "port": 10443
  },
  "schema": {
    "admin_user": "str",
    "admin_email": "email",
    "admin_password": "str",
    "log_level": "list(debug|info|warn|error)?",
    "port": "int(1024,65535)"
  },
  "ports": {
    "3000/tcp": 10443
  },
  "ports_description": {
    "3000/tcp": "Semaphore Web UI"
  },
  "ingress": true,
  "ingress_port": 3000,
  "panel_icon": "mdi:ansible",
  "panel_title": "Semaphore",
  "panel_admin": true,
  "webui": "http://[HOST]:[PORT:10443]",
  "watchdog": "http://[HOST]:[PORT:10443]",
  "environment": {
    "LOG_LEVEL": "info",
    "SEMAPHORE_DB_DIALECT": "bolt",
    "SEMAPHORE_DB": "/share/semaphore/semaphore.db",
    "SEMAPHORE_ADMIN": "admin",
    "SEMAPHORE_ADMIN_EMAIL": "admin@example.com",
    "SEMAPHORE_ADMIN_PASSWORD": "changeme",
    "SEMAPHORE_PLAYBOOK_PATH": "/share/semaphore/playbooks",
    "SEMAPHORE_TMP_PATH": "/share/semaphore/tmp"
  },
  "backup": "hot",
  "stage": "stable"
}