{
  "name": "Semaphore UI",
  "version": "v2.16.17",
  "slug": "semaphore",
  "description": "Ansible/Terraform/OpenTofu UI (Semaphore) as a Home Assistant add-on.",
  "url": "https://github.com/semaphoreui/semaphore",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 3000,
  "panel_icon": "mdi:ansible",
  "panel_title": "Semaphore",
  "map": ["config:rw"],
  "options": {
    "admin_user": "admin",
    "admin_password": "changeme",
    "admin_name": "Admin",
    "admin_email": "admin@localhost",
    "db_dialect": "bolt",
    "timezone": "Etc/UTC"
  },
  "schema": {
    "admin_user": "str",
    "admin_password": "password",
    "admin_name": "str",
    "admin_email": "str",
    "db_dialect": "list(bolt|sqlite|mysql|postgres)",
    "timezone": "str"
  },
  "environment": {
    "SEMAPHORE_DB_DIALECT": "bolt",
    "SEMAPHORE_CONFIG_PATH": "/config/semaphore",
    "SEMAPHORE_DB_PATH": "/config/semaphore-data",
    "SEMAPHORE_TMP_PATH": "/config/semaphore-tmp",
    "SEMAPHORE_DB": "/config/semaphore/semaphore.db"
  },
  "ports": {
    "3000/tcp": null
  },
  "ports_description": {
    "3000/tcp": "Semaphore Web UI"
  },
  "image": "semaphoreui/semaphore"
}