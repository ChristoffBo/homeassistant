
{
  "name": "Ansible Semaphore",
  "version": "v2.16.17",
  "slug": "ansible_semaphore",
  "description": "Modern web UI for Ansible automation using built-in BoltDB. Ingress-enabled.",
  "url": "https://docs.semaphoreui.com/",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 3000,
  "panel_icon": "mdi:ansible",
  "panel_title": "Semaphore",
  "panel_admin": true,
  "map": ["share:rw"],
  "options": {
    "admin_user": "admin",
    "admin_email": "admin@example.com",
    "admin_password": "changeme",
    "log_level": "info",
    "access_key_encryption": ""
  },
  "schema": {
    "admin_user": "str",
    "admin_email": "email",
    "admin_password": "str",
    "log_level": "list(debug|info|warn|error)?",
    "access_key_encryption": "str?"
  },
  "watchdog": "http://[HOST]:[PORT:3000]",
  "backup": "hot"
}
