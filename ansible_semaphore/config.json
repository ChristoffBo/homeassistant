{
  "name": "Ansible Semaphore",
  "version": "v2.16.31",
  "slug": "ansible_semaphore",
  "description": "Semaphore UI inside Home Assistant (official upstream image).",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "ingress": false,
  "ingress_port": 8055,
  "map": ["share:rw"],
  "image": "semaphoreui/semaphore",
  "ports": {
    "8055/tcp": 8055
  },
  "ports_description": {
    "8055/tcp": "Semaphore Web UI"
  },
  "webui": "http://[HOST]:[PORT:8055]",
  "options": {
    "semaphore_port": 8055,
    "semaphore_db_dialect": "sqlite",
    "semaphore_db_host": "/share/ansible_semaphore/semaphore.db",
    "semaphore_tmp_path": "/tmp/semaphore",
    "semaphore_playbook_path": "/share/ansible_semaphore/playbooks",
    "semaphore_admin": "admin",
    "semaphore_admin_name": "Admin",
    "semaphore_admin_email": "admin@example.com",
    "semaphore_admin_password": "ChangeMe!123"
  },
  "schema": {
    "semaphore_port": "int",
    "semaphore_db_dialect": "str",
    "semaphore_db_host": "str",
    "semaphore_tmp_path": "str",
    "semaphore_playbook_path": "str",
    "semaphore_admin": "str",
    "semaphore_admin_name": "str",
    "semaphore_admin_email": "str",
    "semaphore_admin_password": "str"
  }
}