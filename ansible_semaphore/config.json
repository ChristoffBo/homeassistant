{
  "name": "Ansible Semaphore",
  "version": "v2.16.17",
  "slug": "ansible_semaphore",
  "description": "Modern web UI for Ansible automation with SQLite backend. Manage your Ansible playbooks, inventories, and deployments through an intuitive interface.",
  "url": "https://github.com/semaphoreui/semaphore",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "map": [
    "config:rw",
    "ssl:ro"
  ],
  "options": {
    "port": 10443,
    "admin_user": "admin",
    "admin_email": "admin@example.com",
    "admin_password": "changeme",
    "log_level": "info",
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "port": "int(1024,65535)",
    "admin_user": "str",
    "admin_email": "email",
    "admin_password": "str",
    "log_level": "list(debug|info|warn|error)?",
    "ssl": "bool?",
    "certfile": "str?",
    "keyfile": "str?"
  },
  "ports": {
    "3000/tcp": 10443
  },
  "ports_description": {
    "3000/tcp": "Semaphore Web UI"
  },
  "ingress": true,
  "ingress_port": 10443,
  "panel_icon": "mdi:ansible",
  "panel_title": "Semaphore",
  "panel_admin": true,
  "webui": "http://[HOST]:[PORT:10443]",
  "watchdog": "http://[HOST]:[PORT:10443]",
  "environment": {
    "LOG_LEVEL": "info",
    "SEMAPHORE_DB_DIALECT": "bolt",
    "SEMAPHORE_DB": "/data/semaphore.db",
    "SEMAPHORE_ADMIN": "admin",
    "SEMAPHORE_ADMIN_EMAIL": "admin@example.com",
    "SEMAPHORE_ADMIN_PASSWORD": "changeme"
  },
  "backup": "cold",
  "stage": "stable",
  "image": "semaphoreui/semaphore"
}
