{
  "name": "Ansible Semaphore",
  "version": "2.16.17",
  "slug": "ansible_semaphore",
  "description": "Web UI for Ansible playbooks",
  "arch": ["amd64", "arm64"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "3000/tcp": 3000
  },
  "webui": "http://[HOST]:[PORT:3000]",
  "ingress": false,
  "map": ["config", "share", "ssl"],
  "options": {
    "admin_username": "admin",
    "admin_password": "changeme",
    "admin_email": "admin@example.com"
  },
  "schema": {
    "admin_username": "str",
    "admin_password": "str",
    "admin_email": "str"
  },
  "image": "local/ansible_semaphore",
  "stage": "stable"
}