{
  "name": "Ansible Semaphore",
  "version": "v2.16.17",
  "slug": "ansible_semaphore",
  "description": "Semaphore UI inside Home Assistant (no ingress; direct port).",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "host_network": false,
  "ingress": false,
  "panel_admin": false,
  "map": ["share:rw"],
  "image": "semaphoreui/semaphore",
  "ports": {
    "8055/tcp": 8055
  },
  "ports_description": {
    "8055/tcp": "Semaphore Web UI"
  },
  "webui": "http://[HOST]:[PORT:8055]",
  "options": {
    "admin_login": "admin",
    "admin_password": "changeme",
    "config_path": "/share/ansible_semaphore"
  },
  "schema": {
    "admin_login": "str",
    "admin_password": "password",
    "config_path": "str"
  }
}
