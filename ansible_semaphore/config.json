{
  "name": "Ansible Semaphore",
  "version": "v2.16.18",
  "slug": "ansible_semaphore",
  "description": "Semaphore UI for Ansible automation inside Home Assistant",
  "url": "https://github.com/semaphoreui/semaphore",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "host_network": false,
  "ingress": true,
  "ingress_port": 8099,
  "ingress_stream": false,
  "panel_admin": false,
  "panel_icon": "mdi:ansible",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "image": "semaphoreui/semaphore",
  "ports": {
    "8055/tcp": 8055
  },
  "ports_description": {
    "8055/tcp": "Semaphore Web UI (Direct Access)"
  },
  "webui": "http://[HOST]:[PORT:8055]",
  "options": {
    "admin_login": "admin",
    "admin_email": "admin@example.com",
    "admin_name": "Admin",
    "admin_password": "ChangeMe!123",
    "config_path": "/share/ansible_semaphore/config.json",
    "data_dir": "/share/ansible_semaphore",
    "port": 8055
  },
  "schema": {
    "admin_login": "str",
    "admin_email": "email",
    "admin_name": "str",
    "admin_password": "password",
    "config_path": "str",
    "data_dir": "str",
    "port": "port",
    "cookie_hash": "str?",
    "cookie_encryption": "str?",
    "access_key_encryption": "str?"
  }
}
