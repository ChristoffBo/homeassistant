{
  "name": "APT Cacher",
  "version": "1.0",
  "slug": "apt_cacher",
  "description": "APT-Cacher NG - Debian/Ubuntu package cache proxy server",
  "url": "https://www.unix-ag.uni-kl.de/~bloch/acng/",
  "arch": ["amd64", "aarch64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "ingress": false,
  "host_network": true,
  "webui": "http://[HOST]:[PORT:3142]/acng-report.html",
  "panel_icon": "mdi:server-network",
  "panel_title": "APT Cache UI",
  "panel_admin": false,
  "ports": {
    "3142/tcp": 3142
  },
  "ports_description": {
    "3142/tcp": "APT-Cacher NG Web UI and proxy port"
  },
  "map": ["config:rw"],
  "options": {
    "port": 3142
  },
  "schema": {
    "port": "int"
  }
}
