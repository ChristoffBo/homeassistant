{
  "name": "Gitea",
  "version": "1.18.0",
  "slug": "gitea",
  "description": "Self-hosted Git service",
  "url": "https://gitea.io/",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "3000/tcp": 3000,
    "2222/tcp": 2222
  },
  "ports_description": {
    "3000/tcp": "Web Interface",
    "2222/tcp": "SSH Access"
  },
  "map": ["share:rw", "ssl"],
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "list(trace|debug|info|warning|error|fatal)"
  },
  "image": "ghcr.io/gitea/gitea:{arch}-1.18.0"
}