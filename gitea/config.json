{
  "name": "Gitea",
  "version": "1.18.0",
  "slug": "gitea",
  "description": "Self-hosted Git service with painless setup",
  "url": "https://gitea.io",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "image": "gitea/gitea:1.18.0",
  "panel_icon": "mdi:git",
  "panel_title": "Gitea",
  "panel_admin": false,
  "ports": {
    "3000/tcp": 3001,
    "2222/tcp": 2222
  },
  "ports_description": {
    "3000/tcp": "Web interface (http://<host>:3001)",
    "2222/tcp": "SSH access"
  },
  "map": ["share:rw", "ssl"],
  "options": {
    "log_level": "info",
    "ssh_enabled": true,
    "domain": "10.0.0.100",
    "app_name": "Gitea",
    "disable_registration": false
  },
  "schema": {
    "log_level": "list(trace|debug|info|warning|error|fatal)",
    "ssh_enabled": "bool",
    "domain": "str",
    "app_name": "str",
    "disable_registration": "bool"
  }
}