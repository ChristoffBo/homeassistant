{
  "name": "GitHub Uploader",
  "version": "1.0.0",
  "slug": "github_uploader",
  "description": "Upload zipped folders to a GitHub repo with token",
  "startup": "application",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8080]",
  "ingress": true,
  "ingress_port": 8080,
  "host_network": true,
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "list(info|debug|warn|error)"
  },
  "ports": {
    "8080/tcp": 8080
  },
  "image": "ghcr.io/home-assistant/amd64-addon-example"
}