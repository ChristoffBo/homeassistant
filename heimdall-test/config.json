{
  "name": "heimdall-test",
  "version": "1.0",
  "slug": "heimdall-test",
  "legacy": false,
  },
  "description": "Application Dashboard",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "80/tcp": 82,
    "443/tcp": 8445
  },
  "ports_description": {
    "80/tcp": "web interface",
    "443/tcp": "web interface (SSL)"
  },
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:82]",
  "boot": "auto"
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
 }
