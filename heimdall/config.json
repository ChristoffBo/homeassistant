{
  "name": "heimdall",
  "version": "1.0.0",
  "slug": "heimdall",
  "description": "Application Dashboard",
  "url": "",
  "codenotary": "bothmacell@gmail.com",
  "startup": "services",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "ports": {
    "80/tcp": 82,
    "443/tcp": 4445,
  },
  "ports_description": {
    "80/tcp": "web interface",
    "443/tcp": "web interface (SSL)",
  },
  "host_network": true,
  "auth_api": false,
  "hassio_api": false
   ],
  "webui": "http://[HOST]:[PORT:82]",
  "boot": "auto",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
}
