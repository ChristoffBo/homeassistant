{
  "name": "Jarvis Prime",
  "version": "1.1.0",
  "slug": "jarvis_prime",
  "description": "Universal notify orchestrator: ingest (Gotify/ntfy/SMTP), Inbox UI with Ingress, and push fan-out (Gotify/ntfy/Gmail). Includes Beautifier, wake-words, and modules.",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_entry": "/ui/",
  "ports": {
    "2525/tcp": 2525,
    "2580/tcp": 2580,
    "2581/tcp": 2581
  },
  "map": [
    "share:rw",
    "config:rw"
  ],
  "options": {
    "ingest_gotify_enabled": true,
    "ingest_ntfy_enabled": false,
    "ingest_smtp_enabled": true,
    "gotify_url": "http://10.0.0.99:8091",
    "gotify_client_token": "CHANGE_ME_CLIENT",
    "gotify_app_token": "CHANGE_ME_APP",
    "ntfy_url": "http://10.0.0.99:2580",
    "ntfy_topic": "jarvis",
    "ntfy_user": "",
    "ntfy_pass": "",
    "ntfy_token": "",
    "smtp_host": "0.0.0.0",
    "smtp_port": 2525,
    "push_gotify_enabled": true,
    "push_ntfy_enabled": false,
    "push_smtp_enabled": false,
    "push_smtp_host": "smtp.gmail.com",
    "push_smtp_port": 587,
    "push_smtp_user": "you@gmail.com",
    "push_smtp_pass": "APP_PASSWORD",
    "push_smtp_to": "target@gmail.com",
    "retention_days": 30,
    "auto_purge_policy": "off"
  },
  "schema": {
    "ingest_gotify_enabled": "bool",
    "ingest_ntfy_enabled": "bool",
    "ingest_smtp_enabled": "bool",
    "gotify_url": "str",
    "gotify_client_token": "str",
    "gotify_app_token": "str",
    "ntfy_url": "str",
    "ntfy_topic": "str",
    "ntfy_user": "str",
    "ntfy_pass": "str",
    "ntfy_token": "str",
    "smtp_host": "str",
    "smtp_port": "int",
    "push_gotify_enabled": "bool",
    "push_ntfy_enabled": "bool",
    "push_smtp_enabled": "bool",
    "push_smtp_host": "str",
    "push_smtp_port": "int",
    "push_smtp_user": "str",
    "push_smtp_pass": "str",
    "push_smtp_to": "str",
    "retention_days": "int",
    "auto_purge_policy": "str"
  }
}