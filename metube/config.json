{
  "arch": [
    "amd64",
    "aarch64"
  ],
  "boot": "auto",
  "description": "Web GUI for youtube-dl",
  "environment": {
    "DOWNLOAD_DIR": "/share/metube/"
  },
  "map": [
    "share:rw"
    "devices": [
    "/dev/dri",
    "/dev/vchiq",
    "/dev/video10",
    "/dev/video11",
    "/dev/video12",
    "/dev/video13",
    "/dev/video14",
    "/dev/video15",
    "/dev/video16",
    "/dev/ttyUSB0",
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2" 
  ],
  "version": "2022-11-19",
  "name": "metube",
  "options": {},
  "ports": {
    "8081/tcp": "8081"
  },
  "ports_description": {
    "8081/tcp": "Web interface"
  },
  "schema": {},
   "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "claim": "str",
    "localdisks": "str?",
    "networkdisks": "str?",
    "smbv1": "bool?"
  "slug": "metube",
  "startup": "services",
  "upstream": "",
  "url": "https://github.com/jdeath/homeassistant-addons",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8081]",
  "ingress": true,
  "ingress_port": 8081
}
