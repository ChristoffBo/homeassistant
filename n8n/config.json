  "schema": {
    "port": "port",
    "timezone": "str",
    "basic_auth_active": "bool",
    "basic_auth_user": "str?",
    "basic_auth_password": "str?"
  },
  "ports": {
    "5678/tcp": 5678
  },
  "map": ["config:rw", "share:rw", "ssl"]
}