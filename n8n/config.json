{
  "name": "n8n Workflow Automation",
  "version": "1.48.0",
  "slug": "n8nio",
  "description": "n8n is an extendable workflow automation tool for connecting APIs and services.",
  "url": "https://n8nio/",
  "startup": "services",
  "boot": "auto",
  "init": false,
  "image": "docker.io/n8nio/n8n:1.48.0",
  "arch": ["amd64", "armv7", "aarch64"],
  "host_network": true,
  "map": ["config:rw", "ssl", "share:rw", "addons:rw"],
  "options": {
    "timezone": "Europe/London",
    "auth_user": "",
    "auth_pass": ""
  },
  "schema": {
    "timezone": "str",
    "auth_user": "str?",
    "auth_pass": "str?"
  },
  "environment": {
    "N8N_PORT": "5678",
    "N8N_HOST": "0.0.0.0",
    "N8N_BASIC_AUTH_ACTIVE": "true",
    "N8N_BASIC_AUTH_USER": "!auth_user",
    "N8N_BASIC_AUTH_PASSWORD": "!auth_pass",
    "TZ": "!timezone"
  },
  "ports": {
    "5678/tcp": 5678
  }
}