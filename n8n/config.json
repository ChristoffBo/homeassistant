{
  "name": "n8n",
  "version": "1.48.0",
  "slug": "n8n",
  "description": "Extendable workflow automation tool for connecting APIs and services.",
  "url": "https://n8n.io/",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "host_network": false,
  "options": {
    "port": 5678,
    "timezone": "Europe/London",
    "basic_auth_active": false,
    "basic_auth_user": "",
    "basic_auth_password": ""
