{
  "name": "Portainer",
  "version": "2.34.0-alpine",
  "slug": "portainer",
  "description": "Portainer CE - Manage your Docker containers and hosts easily via UI.",
  "url": "https://www.portainer.io",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:9000]",
  "ingress": true,
  "ingress_port": 9000,
  "panel_icon": "mdi:docker",
  "panel_title": "Portainer",
  "panel_admin": false,
  "ports": {
    "9000/tcp": 9000
  },
  "ports_description": {
    "9000/tcp": "Portainer Web UI (if not using Ingress)"
  },
  "map": [
    "config:rw"
  ],
  "options": {
    "port": 9000,
    "cli_args": "",
    "image_override": "portainer/portainer-ce"
  },
  "schema": {
    "port": "int",
    "cli_args": "str",
    "image_override": "str"
  },
  "image": "portainer/portainer-ce",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ]
}
