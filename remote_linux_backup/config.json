{
  "name": "Remote Linux Backup",
  "version": "1.6.3",
  "slug": "remote_linux_backup",
  "description": "Backup/restore over SSH, SMB/NFS (with fallback), image (dd). Wizard UI. Duplicati-inspired.",
  "url": "https://github.com/ChristoffBo/homeassistant",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf",
    "i386"
  ],
  "host_network": false,
  "ports": {
    "8066/tcp": null
  },
  "ingress": false,
  "panel_icon": "mdi:backup-restore",
  "apparmor": false,
  "privileged": [
    "SYS_ADMIN"
  ],
  "map": [
    "config:rw",
    "share:rw"
  ],
  "options": {
    "port": 8066
  },
  "schema": {
    "port": "int"
  }
}