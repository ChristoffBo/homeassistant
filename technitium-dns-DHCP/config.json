{
  "name": "TechNitium DNS",
  "version": "1.0.0",
  "slug": "technitium_dns",
  "description": "TechNitium DNS server for managing DNS requests",
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "arch": ["amd64", "armv7", "aarch64"],
  "ports": {
    "53/tcp": 53,
    "443/tcp": 443,
    "80/tcp": 80
  },
  "options": {
    "dns_port": 53,
    "dns_ipv4": "0.0.0.0",
    "dns_ipv6": "::",
    "dns_web_port": 80,
    "dns_https_port": 443
  },
  "schema": {
    "dns_port": "int",
    "dns_ipv4": "str",
    "dns_ipv6": "str",
    "dns_web_port": "int",
    "dns_https_port": "int"
  },
  "image": "technitium/dns-server:latest",
  "volume": "/config/technitium-dns",
  "map": [
    "/config/technitium-dns:/app/dnsserver"
  ],
  "support": [
    "email@example.com"
  ],
  "init": false,

  "panel_iframe": {
    "title": "TechNitium DNS",
    "icon": "mdi:dns",
    "url": "http://localhost:80" 
  }
}

