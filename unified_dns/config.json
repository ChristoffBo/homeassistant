{
  "name": "Unified DNS",
  "version": "1.3.3",
  "slug": "unified_dns",
  "description": "Unified DNS stats + manual sync across Technitium, AdGuard, Pi-hole. Soft-color graphs. Gotify alerts. Cache builder & cache prep. Self-check & failsafes.",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_port": 8067,
  "ingress_entry": "index.html",
  "panel_icon": "mdi:dns",
  "panel_title": "Unified DNS",
  "host_network": true,
  "webui": "http://[HOST]:[PORT:8067]",
  "map": ["config:rw", "share:rw"],
  "options": {
    "listen_port": 8067,
    "gotify_url": "",
    "gotify_token": "",
    "cache_builder_list": [
      "google.com",
      "microsoft.com",
      "apple.com",
      "youtube.com",
      "netflix.com"
    ],
    "servers": []
  },
  "schema": {
    "listen_port": "int",
    "gotify_url": "str?",
    "gotify_token": "str?",
    "cache_builder_list": ["str"],
    "servers": [
      {
        "name": "str",
        "type": "list(technitium|adguard|pihole)",
        "base_url": "str",
        "dns_host": "str?",
        "dns_port": "int?",
        "dns_protocol": "list(udp|tcp)?",
        "username": "str?",
        "password": "str?",
        "token": "str?",
        "verify_tls": "bool?",
        "primary": "bool",
        "cache_builder_override": "bool?",
        "cache_builder_list": ["str"]
      }
    ]
  }
}