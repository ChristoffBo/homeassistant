{
  "name": "Home Assistant Addons Docker Version Updater",
  "version": "1.0.0",
  "slug": "addons_version_updater",
  "description": "Checks and updates add-on version numbers to match latest Docker image tags",
  "url": "https://github.com/ChristoffBo/homeassistant",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "amd64",
    "armv7",
    "aarch64"
  ],
  "ports": {},
  "options": {
    "github_repo": "https://github.com/yourrepo/homeassistant",
    "github_username": "",
    "github_token": "",
    "check_cron": "0 3 * * *",
    "startup_cron": "0 4 * * *",
    "timezone": "Africa/Johannesburg",
    "max_log_lines": 1000,
    "dry_run": false,
    "skip_push": false,
    "notifications_enabled": false,
    "notification_service": "gotify",
    "notification_url": "",
    "notification_token": "",
    "notification_to": "",
    "notify_on_success": true,
    "notify_on_error": true,
    "notify_on_updates": true,
    "persistent": true
  },
  "schema": {
    "github_repo": "str",
    "github_username": "str",
    "github_token": "str",
    "check_cron": "str",
    "startup_cron": "str?",
    "timezone": "str?",
    "max_log_lines": "int?",
    "dry_run": "bool?",
    "skip_push": "bool?",
    "notifications_enabled": "bool?",
    "notification_service": "str?",
    "notification_url": "str?",
    "notification_token": "str?",
    "notification_to": "str?",
    "notify_on_success": "bool?",
    "notify_on_error": "bool?",
    "notify_on_updates": "bool?",
    "persistent": "bool?"
  },
  "map": [
    "config:ro",
    "addons:rw",
    "backup:rw",
    "share:rw"
  ],
  "init": false,
  "host_network": false,
  "privileged": []
}