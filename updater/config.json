{
  "name": "Home Assistant Addons Docker Version Updater",
  "version": "1.0.0",
  "slug": "addons_version_updater",
  "description": "Checks and updates add-on version numbers to match latest Docker image tags",
  "url": "https://github.com/ChristoffBo/homeassistant",
  "startup": "services",
  "boot": "auto",
  "arch": ["amd64", "armv7", "aarch64"],
  "ports": {},
  "options": {
    "github_repo": "https://github.com/ChristoffBo/homeassistant.git",
    "github_username": "",
    "github_token": "",
    "check_time": "03:00"
  },
  "schema": {
    "github_repo": "str",
    "github_username": "str",
    "github_token": "str",
    "check_time": "str"
  },
  "map": ["config:ro", "addons:rw", "backup:rw", "share:rw"],
  "init": false,
  "host_network": false,
  "privileged": []
}
