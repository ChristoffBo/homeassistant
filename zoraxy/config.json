{
  "name": "Zoraxy",
  "version": "v3.2.8",
  "slug": "zoraxy",
  "description": "A reverse proxy and dashboard powered by Docker.",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "url": "https://github.com/tobychui/zoraxy",
  "ports": {
    "8080/tcp": 8080,
    "8000/tcp": 8000,
    "443/tcp": 443
  },
  "map": [
    "config:rw",
    "data:rw",
    "ssl:rw"
  ],
  "panel_icon": "mdi:reverse-proxy",
  "panel_title": "Zoraxy",
  "ingress": true,
  "ingress_port": 8080,
  "ingress_stream": true,
  "host_network": false,
  "privileged": [],
  "environment": {},
  "options": {},
  "schema": {},
  "mounts": [
    {
      "container_path": "/opt/zoraxy/config",
      "source": "data",
      "target": "zoraxy"
    },
    {
      "container_path": "/opt/zoraxy/ssl",
      "source": "ssl"
    }
  ]
}
