{
  "name": "ZeroTier Network Controller UI",
  "version": "1.2.18",
  "slug": "ztncui",
  "description": "Self-hosted ZeroTier Controller with web UI using ztncui.",
  "url": "https://github.com/keynetworks/ztncui",
  "arch": [
    "amd64",
    "aarch64",
    "armv7"
  ],
  "startup": "services",
  "boot": "auto",
  "ingress": false,
  "host_network": true,
  "webui": "http://[HOST]:[PORT:3443]",
  "ports": {
    "3443/tcp": 3443
  },
  "ports_description": {
    "3443/tcp": "Ztncui Web UI Port"
  },
  "map": [
    "config:rw",
    "ssl",
    "addons:rw",
    "backup:rw",
    "share:rw"
  ],
  "options": {
    "port": 3443,
    "image": "keynetworks/ztncui"
  },
  "schema": {
    "port": "int",
    "image": "str"
  }
}